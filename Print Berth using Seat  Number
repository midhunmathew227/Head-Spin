#include<stdio.h>
int main()
{ int n;
  printf("\n Enter Seat no. : ");
  scanf("%d",&n);
  printf("\n Berth is ");
  if(n>=1&&n<=72)
  { if(n%8==0)
    { printf("Side Upper (SU)");
    }
    else if(n%8==7)
    { printf("Side Lower (SL)");
    }
    else if(n%8==4||n%8==1)
    { printf("Lower (L)");
    }
    else if(n%8==5||n%8==2)
    { printf("Middle (M)");
    }
    else
    { printf("Upper (U)");
    }
  }
  return 0;
}
