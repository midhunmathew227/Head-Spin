#include <stdio.h>
#include <math.h>

int main()
{ int n, sum, end, dig, num, i;
  printf("Enter Limit : ");
  scanf("%d", &n);
  printf("Armstrong numbers less than %d are : \n", n);
  for(i=1; i<=n; i++)
  { sum = 0;
    num = i;
    dig = (int) log10(num) + 1;

    while(num > 0)
    { end = num % 10;
      sum = sum + ceil(pow(end, dig));
      num = num / 10;
    }
    
    if(i == sum)
    { printf("%d, ", i);
    }

  }
  return 0;
}
